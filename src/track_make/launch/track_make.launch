<?xml version="1.0" encoding="UTF-8"?>  
<launch>
    <arg name="velocity_coeff" default="1"/>

<!--turtlesim_node-->
    <node pkg="turtlesim" type="turtlesim_node" name="sim"/>

<!--aruco_broadcaster-->
    <node pkg="track_make" type="aruco_broadcaster" name="odom_cam_br"/>

<!--aruco_listener-->
    <node pkg="track_make" type="aruco_listener"    name="follow_aruco" output="screen">
    	<param name="Velocity_coeff" value="$(arg velocity_coeff)"/>
    </node>

  </launch> 
